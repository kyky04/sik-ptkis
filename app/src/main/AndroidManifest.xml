<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uinbdg.SIK.ptkis">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name="uinbdg.SIK.ptkis.Activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="uinbdg.SIK.ptkis.Activities.LoginActivity" />
        <activity android:name="uinbdg.SIK.ptkis.Activities.MainActivity" />
        <activity android:name="uinbdg.SIK.ptkis.Activities.DaftarActivity" />

        <activity android:name="uinbdg.SIK.ptkis.Activities.UniversitasActivity" />
        <activity android:name="uinbdg.SIK.ptkis.Activities.JurusanActivity" />
        <activity android:name="uinbdg.SIK.ptkis.Activities.FakultasActivity" />
        <activity android:name="uinbdg.SIK.ptkis.Activities.BeritaUniversitasActivity" />
        <activity android:name="uinbdg.SIK.ptkis.Activities.BeritaFakultasActivity" />
        <activity android:name="uinbdg.SIK.ptkis.Activities.PendaftarActivity" />
    </application>

</manifest>